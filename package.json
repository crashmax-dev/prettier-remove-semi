{
  "name": "prettier-remove-semi",
  "version": "1.0.0",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test:jsx": "prettier --semi=false --plugin . mock/jsx.tsx",
    "test:iife": "prettier --semi=false --plugin . mock/iife.ts"
  },
  "devDependencies": {
    "@crashmax/tsconfig": "^1.0.2",
    "@types/prettier": "^2.7.0",
    "prettier": "^2.7.1",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "prettier": ">=2.0.0"
  }
}
